
public class BattleTest {
	public static void main(String[] args){
		Player p = new Player();
		Monster m = new Monster();

		while(p.hp > 0 && m.hp > 0){
			int a = m.attack();
			int b = p.attack();
			
			p.hp = p.hp - a;
			System.out.println("플레이어가" +a+"의 피해를 입음");
			System.out.println("플레이어 HP:"+p.hp);
			
		}
			
	}
}
